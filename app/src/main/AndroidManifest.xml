<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.jtoru.project2">

    <application
            android:allowBackup="true"
            android:icon="@drawable/logo"
            android:label="@string/app_name"
            android:roundIcon="@mipmap/ic_launcher_round"
            android:supportsRtl="true"
            android:theme="@style/AppTheme">
        <activity android:name=".Actitivies.ImageActivity"
                  android:theme="@style/AppTheme.NoActionBar"
                  android:screenOrientation="portrait">
        </activity>
        <activity
                android:name=".Actitivies.AddPostActivity"
                android:screenOrientation="portrait">
        </activity>
        <activity
                android:name=".Actitivies.MyFriendsActivity"
                android:screenOrientation="portrait">
        </activity>
        <activity
                android:name=".Actitivies.SearchTestActivity"
                android:screenOrientation="portrait">
        </activity>
        <activity
                android:name=".Actitivies.SearchActivity"
                android:screenOrientation="portrait">
        </activity>
        <activity
                android:name=".Actitivies.RegisterActivity"
                android:screenOrientation="portrait">
        </activity>
        <activity
                android:name=".Actitivies.HomeActivity"
                android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity
                android:name=".Actitivies.FriendsActivity"
                android:screenOrientation="portrait">
        </activity>
        <activity
                android:name=".Actitivies.ProfileDetailsActivity"
                android:screenOrientation="portrait">
        </activity>
        <activity
                android:name=".Actitivies.ProfileActivity"
                android:screenOrientation="portrait">
        </activity>
        <activity
                android:name=".Actitivies.MainActivity"
                android:screenOrientation="portrait">
        </activity>
    </application>

</manifest>